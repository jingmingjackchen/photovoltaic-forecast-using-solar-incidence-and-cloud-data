    "### Load weather data from JSON"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "list_of_files = glob.glob('data/*')\n",
    "latest_file = max(list_of_files, key=os.path.getctime)\n",
    "\n",
    "with open(latest_file, \"r\") as f:\n",
    "    data = f.read()\n",
    "    data = json.loads(data)\n",
    "    print(data)"
   ]
  }
